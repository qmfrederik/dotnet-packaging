<Project>
  <!-- <Import Project="..\..\..\version.props" />  -->
  <PropertyGroup>
    <OutputType>None</OutputType>
  </PropertyGroup>
  <Target Name="PublishTestProjects">
       <MSBuild Projects="../../testprojects/clifdd/clifdd.csproj" 
       Targets="Clean;Restore;PackageRpm"
       Properties="RuntimeIdentifier=centos-x64">
          <Output TaskParameter="TargetOutputs" ItemName="FddPackage" /> 
       </MSBuild>
       <MSBuild Projects="../../testprojects/cliscd/cliscd.csproj" 
       Targets="Clean;Restore;PackageRpm"
       Properties="RuntimeIdentifier=centos-x64">
          <Output TaskParameter="TargetOutputs" ItemName="ScdPackage" /> 
       </MSBuild>
  </Target>
  <Target Name="Build" DependsOnTargets="PublishTestProjects">
      <Exec Command="build.cmd &quot;@(FddPackage)&quot; &quot;@(ScdPackage)&quot;" Condition="'$(OS)'=='Windows_NT'"/>
      <Exec Command="bash ./build.sh &quot;@(FddPackage)&quot; &quot;@(ScdPackage)&quot;" Condition="'$(OS)'=='Unix'"/>
  </Target>
  <Target Name="VSTest" DependsOnTargets="Build"> 
      <Exec Command="test.cmd &quot;@(FddPackage)&quot; &quot;@(ScdPackage)&quot;"  Condition="'$(OS)'=='Windows_NT'"/>
      <Exec Command="bash ./test.sh &quot;@(FddPackage)&quot; &quot;@(ScdPackage)&quot;" Condition="'$(OS)'=='Unix'"/>
  </Target>
  <Target Name="Clean">
      <Exec Command="clean.cmd" Condition="'$(OS)'=='Windows_NT'"/>
      <Exec Command="bash ./clean.sh" Condition="'$(OS)'=='Unix'"/>
  </Target>
   <Target Name="Restore">
  </Target>
</Project>